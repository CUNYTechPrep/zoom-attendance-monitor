{
  "name": "zoom-attendance-monitor",
  "version": "1.0.0",
  "description": "tool to monitor zoom attendance",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"npm run dev --prefix backend\" \"npm run dev --prefix frontend\"",
    "install:all": "npm install --prefix backend && npm install --prefix frontend",
    "build": "npm run build --prefix frontend",
    "start": "npm run start --prefix backend",
    "lint:be": "eslint ./backend --ext .js,.jsx,.ts,.tsx",
    "lint:fe": "eslint --config ./frontend/eslint.config.js ./frontend --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "prettier:ci": "prettier . --check",
    "format": "prettier --write .",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "engines": {
    "node": "22.14.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/CUNYTechPrep/zoom-attendance-monitor.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/CUNYTechPrep/zoom-attendance-monitor/issues"
  },
  "homepage": "https://github.com/CUNYTechPrep/zoom-attendance-monitor#readme",
  "dependencies": {
    "concurrently": "^9.1.2"
  },
  "devDependencies": {
    "@eslint/compat": "^1.2.7",
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.23.0",
    "eslint": "^9.23.0",
    "eslint-config-prettier": "^10.1.1",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^5.2.3",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-react-hooks": "^5.2.0",
    "globals": "^16.0.0",
    "prettier": "^3.5.3"
  }
}
