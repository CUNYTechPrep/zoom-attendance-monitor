{
  "name": "backend",
  "version": "1.0.0",
  "engines": {
    "node": "22.14.0"
  },
  "description": "REST API backend for the zoom attendance monitor",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "format": "prettier . --check",
    "lint": "eslint .",
    "start": "node app.js",
    "test": "NODE_OPTIONS=--experimental-vm-modules jest",
    "test:ci": "NODE_OPTIONS=--experimental-vm-modules jest --ci --colors",
    "dev": "node --watch --env-file=.env index.js",
    "build": "cd ../frontend && npm install && npm run build",
    "generate:model": "npx sequelize-cli model:generate",
    "generate:migration": "npx sequelize-cli migration:generate --name",
    "generate:seed": "npx sequelize-cli seed:generate --name",
    "migrate": "npx sequelize-cli db:migrate",
    "seed": "npx sequelize-cli db:seed:all"
  },
  "author": "CUNY Tech Prep",
  "license": "MIT",
  "dependencies": {
    "@types/jest": "^29.5.14",
    "dotenv": "^16.4.7",
    "express": "4.21.1",
    "jest": "^29.7.0",
    "morgan": "^1.10.0",
    "pg": "^8.12.0",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.37.3",
    "sqlite3": "^5.1.7"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.26.9",
    "@eslint/js": "^9.9.1",
    "@types/express": "4.17.21",
    "babel-jest": "^29.7.0",
    "babel-register": "^6.26.0",
    "eslint": "^9.9.1",
    "eslint-config-prettier": "9.1.0",
    "globals": "^15.9.0",
    "prettier": "3.3.3",
    "supertest": "^7.1.0"
  }
}
